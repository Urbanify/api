# HTTP server port (default 3000)
SERVER_PORT=
# JWT signing secret used to issue/verify tokens
JWT_SECRET=
# Prisma primary database connection string
DATABASE_URL=
# Prisma direct/maintenance connection (optional, often points to same DB)
DIRECT_URL=
# Internal API key used to secure private endpoints
API_KEY=
# Mailgun API key
MAILGUN_KEY=
# Mailgun API base URL (e.g. https://api.mailgun.net)
MAILGUN_URL=
# Default sender address for emails
MAIL_SERVICE_FROM=
# Frontend base URL for CORS/redirects
FRONTEND_URL=
